                    <tr><td>null</td> <td>stream</td> <td>serialization-state</td> <td></td></tr>
                    <tr><td>(eql 't)</td> <td>stream</td> <td>serialization-state</td> <td></td></tr>
                    <tr><td>string</td> <td>stream</td> <td>serialization-state</td> <td></td></tr>
